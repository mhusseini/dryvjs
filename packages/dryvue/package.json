  "dependencies": {
    "dryvjs": "workspace:^1.0.0-pre-25"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.5",
    "concurrently": "^8.2.2",
    "eslint": "^8.49.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "tsc-alias": "^1.8.8",
    "typescript": "~5.2.0"
  }
